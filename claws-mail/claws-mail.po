msgstr "  --offline              切换至离线模式"

msgid "  --online               switch to online mode"
msgstr "  --online               切换至上线模式"

msgid "  --receive              receive new messages"
msgstr "  --receive              接收新邮件"

msgid "  --receive-all          receive new messages of all accounts"
msgstr "  --receive-all          接收所有帐号的新邮件"

msgid "  --reset-statistics     reset session statistics"
msgstr "  --reset-statistics     重置统计信息"

msgid ""
"  --search folder type request [recursive]\n"
"                         searches mail\n"
"                         folder ex.: \"#mh/Mailbox/inbox\" or \"Mail\"\n"
"                         type: s[ubject],f[rom],t[o],e[xtended],m[ixed] or "
"g: tag\n"
"                         request: search string\n"
"                         recursive: false if arg. starts with 0, n, N, f or F"
msgstr ""
"  --search 需求的文件夹型式 [递归]\n"
"                         搜索邮件\n"
"                         文件夹 例如： \"#mh/Mailbox/inbox\" or \"Mail\"\n"
"                         型式： s[主旨],f[发件人],t[收件人],e[延伸],m[混合] "
"或 g: 标签\n"
"                         需求： 搜索字串\n"
"                         递归: 若字串的开头为 0, n, N, f or F，将会失败"

msgid ""
"  --select folder[/msg]  jumps to the specified folder/message\n"
"                         folder is a folder id like 'folder/sub_folder'"
msgstr ""
"  --select 文件夹[/邮件]  跳至指定的文件夹/邮件\n"
"                         文件夹是一个文件夹 id 像是'文件夹/子文件夹(_f)'"

msgid "  --send                 send all queued messages"
msgstr "  --send                 发送所有草稿邮件"

msgid "  --statistics           show session statistics"
msgstr "  --statistics           显示统计信息"

msgid "  --status [folder]...   show the total number of messages"
msgstr "  --status [文件夹]...   显示所有邮件的数量"

msgid ""
"  --status-full [folder]...\n"
